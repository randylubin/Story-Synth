{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
        "source": "**",
        "destination": "/app.html"
        },
        {
          "source": "/",
          "destination": "/index.html"
        }
    ]
  }
}
